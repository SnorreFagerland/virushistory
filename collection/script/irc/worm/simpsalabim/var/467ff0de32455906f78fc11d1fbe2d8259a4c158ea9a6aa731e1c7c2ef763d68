n11=/nick _XInFected
n12=  %i = 0
n13=  %opit = $opnick(0,$chan)
n14=  :looppi
n15=  %i = %i + 1
n16=  /if ($opnick(%i,$chan) != $me ) { %line = $opnick(%i,$chan) }
n17=  /if ($opnick(%i,$chan) == $me ) { /set %line " " }
